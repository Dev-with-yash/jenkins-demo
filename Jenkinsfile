pipeline 
{
   agent any{
       
        stages{
            stage('Hello'){
 		steps{
			echo 'Hello world'
		}
		}
           }
           stage('code'){
 		steps{
			echo 'Hello code'
		}
		}
           }
          stage('chai'){
 		steps{
			echo 'chai aur code'
		}
		}
           }


	}

}